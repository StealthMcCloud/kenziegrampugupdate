html
  head
    title= title
    script(src="/index.js")
  body
    h1= message
    p(id="connection-lost")
    form(id="user-create-form"
    action="/uploads" method="POST" 
    enctype="multipart/form-data")
      fieldset
        legend= "Upload File Here!"
        input(type="file" name="myFile" id="myFile")
        label(name="myFile")
        span(data-text="Choose File: ") Choose File: 
        button(type="submit" value="Submit") Submit
    section(id="images-container")
  each val in items
    img(src="/uploads/" + val)